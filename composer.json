{
    "name": "ocdladefense/company-wordpress",
    "description": "A wordpress install for the company.",
    "minimum-stability": "dev",
    "type":"wordpress-theme",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "extra": {
    	"installer-paths": {
            "wp-content/plugins/composer":["ocdladefense/wp-composer"],
            "wp-content/plugins/salesforce":["ocdladefense/wp-salesforce"],
            "wp-content/themes/company":["ocdladefense/company-theme"]
    	}
    },
    "require": {
        "composer/installers":"~1.0",
        "ocdladefense/wp-composer":"dev-main",
        "ocdladefense/company-theme":"dev-development"
    }
}
